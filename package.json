{
  "name": "@derekkent/derekkent.com",
  "version": "0.1.0",
  "description": "Derek Kent for Maryland District 32 Delegate official web site",
  "scripts": {
    "prepublish": "jspm install",
    "upgrade": "jspm update",
    "test": "gulp build && gulp test"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/DerekKent/derekkent.com.git"
  },
  "license": "CC0-1.0",
  "devDependencies": {
    "ava": "0.16.0",
    "babel-eslint": "7.1.0",
    "babel-plugin-transform-class-properties": "6.18.0",
    "babel-plugin-transform-decorators": "6.13.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-preset-es2015": "6.18.0",
    "browser-sync": "2.17.6",
    "connect-history-api-fallback": "1.3.0",
    "del": "2.2.2",
    "eslint": "3.10.0",
    "find-pkg": "0.1.2",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-autoprefixer": "3.1.1",
    "gulp-ava": "0.14.1",
    "gulp-babel": "6.1.2",
    "gulp-clip-empty-files": "0.1.2",
    "gulp-cssnano": "2.1.2",
    "gulp-env": "0.4.0",
    "gulp-eslint": "3.0.1",
    "gulp-htmlmin": "3.0.0",
    "gulp-humans": "2.0.2",
    "gulp-if": "2.0.2",
    "gulp-imagemin": "3.1.1",
    "gulp-load-plugins": "1.4.0",
    "gulp-real-favicon": "0.2.1",
    "gulp-rename": "1.2.2",
    "gulp-replace": "0.5.4",
    "gulp-sass": "2.3.2",
    "gulp-scss-lint": "0.4.0",
    "gulp-sourcemaps": "2.2.0",
    "gulp-superviewsjs": "github:dak/gulp-superviewsjs",
    "gulp-uglify": "2.0.0",
    "gulp-util": "3.0.7",
    "gulp-wrap": "0.13.0",
    "jsdom": "9.8.3",
    "jsdom-global": "2.1.0",
    "jspm": "0.17.0-beta.32",
    "require-dir": "0.3.1",
    "sw-precache": "4.2.1",
    "yargs": "6.3.0"
  },
  "jspm": {
    "main": "src/app/main.js",
    "directories": {
      "lib": "src/app"
    },
    "configFiles": {
      "jspm": "src/jspm.config.js",
      "jspm:dev": "src/jspm.dev.js",
      "jspm:browser": "src/jspm.browser.js"
    },
    "dependencies": {
      "fetch": "npm:whatwg-fetch@^1.0.0",
      "superb": "npm:superb.js@^0.2.10"
    },
    "devDependencies": {},
    "peerDependencies": {
      "incremental-dom": "npm:incremental-dom@^0.4.1"
    },
    "overrides": {}
  },
  "ava": {
    "require": [
      "./test/helpers/jspm_loader",
      "./test/helpers/setup-browser-env.js"
    ],
    "verbose": true
  },
  "nyc": {
    "include": [
      "dev/**/*.js"
    ],
    "exclude": [
      "dist",
      "gulp",
      "jspm_packages",
      "node_modules",
      "src",
      "test"
    ]
  },
  "engines": {
    "node": ">=6.2"
  }
}
